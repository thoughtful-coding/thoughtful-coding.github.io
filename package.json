{
  "name": "thoughtful-python-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:beta": "export $(grep -v '^#' .env.test | xargs) && vite",
    "build": "NODE_ENV=production node scripts/fetch-courses.js && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "e2e-test": "playwright test",
    "e2e-test:ci": "playwright test --grep-invert @flaky",
    "e2e-codegen": "export $(grep -v '^#' .env.test | xargs) && playwright codegen http://localhost:5173",
    "format": "prettier --write \"{src,e2e}/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"{src,e2e}/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.css\" --fix",
    "stylelint:report": "stylelint \"src/**/*.css\" --formatter verbose > stylelint-report.txt || true"
  },
  "dependencies": {
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@react-oauth/google": "^0.12.2",
    "@types/p5": "^1.7.6",
    "@types/pixelmatch": "^5.2.6",
    "@uiw/react-codemirror": "^4.23.12",
    "immer": "^10.2.0",
    "p5": "^2.0.5",
    "pixelmatch": "^7.1.0",
    "react": "^19.1.0",
    "react-devtools": "^6.1.2",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.0",
    "real-turtle": "^1.1.6",
    "remark-gfm": "^4.0.1",
    "uuid": "^11.1.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-order": "^7.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.0.0",
    "vitest": "^3.2.4"
  }
}
